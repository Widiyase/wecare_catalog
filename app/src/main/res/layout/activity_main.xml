<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="96dp"
        android:layout_height="73.21dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/logo" />

    <androidx.constraintlayout.utils.widget.ImageFilterButton
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="195dp"
        android:contentDescription="@string/app_name"
        app:srcCompat="@drawable/rectangle_1" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="194dp"
        android:layout_height="174dp"
        android:layout_alignStart="@id/imageView4"
        android:layout_alignBottom="@id/imageView4"
        android:layout_marginStart="3dp"
        android:layout_marginBottom="1dp"
        app:srcCompat="@drawable/capture_removebg_preview_1" />

    <androidx.constraintlayout.utils.widget.ImageFilterButton
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="400dp"
        android:contentDescription="@string/app_name"
        app:srcCompat="@drawable/rectangle_2" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="249dp"
        android:layout_height="113dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="459dp"
        app:srcCompat="@drawable/image_removebg_preview_1"
        />

    <ImageView
        android:id="@+id/img"
        android:layout_width="134dp"
        android:layout_height="48dp"
        android:layout_marginStart="210dp"
        android:layout_marginTop="215dp"
        app:srcCompat="@drawable/img"
        />

</RelativeLayout>